<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="vm"
            type="com.tech.arch.ui.home_screens.location.LocationFragmentVm" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/consFragMain"
        android:onClick="@{v ->vm.onClick(v)}"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.home_screens.location.LocationFragment">



        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/viewPager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:onClick="@{v -> vm.onClick(v)}"
            android:orientation="vertical" />

        <View
            android:id="@+id/viewPagerClickOverlay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"
            android:clickable="true" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etSearch"
            style="@style/TextView.Regular"
            android:layout_width="0dp"
            android:layout_height="@dimen/_35sdp"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bg_cards"
            android:backgroundTint="@color/white"
            android:hint="search here"
            android:textColorHint="@color/app_blue"
            android:textColor="@color/black"
            android:paddingHorizontal="@dimen/_10sdp"
            android:singleLine="true"
            android:textSize="@dimen/_12ssp"
            app:layout_constraintEnd_toStartOf="@id/ivFilter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/homeProfile"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:src="@drawable/home_profile_icon"
            android:layout_marginEnd="@dimen/_15sdp"
            android:onClick="@{ v -> vm.onClick(v)}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/etSearch"
            app:layout_constraintBottom_toBottomOf="@id/etSearch"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ivFilter"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:src="@drawable/iv_filter_icon"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/homeProfile"
            app:layout_constraintBottom_toBottomOf="@id/homeProfile"
            app:layout_constraintEnd_toStartOf="@id/homeProfile"/>

<!--        <LinearLayout-->
<!--            android:id="@+id/linearItems"-->
<!--            android:layout_width="0dp"-->
<!--            android:layout_height="wrap_content"-->
<!--            android:layout_marginTop="@dimen/_15sdp"-->
<!--            android:background="@drawable/bg_cards"-->
<!--            android:backgroundTint="@color/bottomNavColor"-->
<!--            android:gravity="center_horizontal"-->
<!--            android:visibility="visible"-->
<!--            android:paddingBottom="@dimen/_10sdp"-->
<!--            android:orientation="vertical"-->
<!--            app:layout_constraintStart_toStartOf="@id/homeProfile"-->
<!--            app:layout_constraintEnd_toEndOf="@id/homeProfile"-->
<!--            app:layout_constraintTop_toBottomOf="@id/homeProfile">-->


            <ImageView
                android:id="@+id/iv_folder"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"
                app:layout_constraintTop_toTopOf="@id/linearItems"
                app:layout_constraintStart_toStartOf="@id/homeProfile"
                app:layout_constraintEnd_toEndOf="@id/homeProfile"
                android:contentDescription="@string/rectangle"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_files" />

            <ImageView
                android:id="@+id/iv_bookmark"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                android:contentDescription="@string/rectangle"
                app:layout_constraintTop_toBottomOf="@id/iv_folder"
                app:layout_constraintStart_toStartOf="@id/iv_folder"
                app:layout_constraintEnd_toEndOf="@id/iv_folder"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_bookmark" />

            <ImageView
                android:id="@+id/iv_share"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                android:contentDescription="@string/rectangle"
                android:onClick="@{v -> vm.onClick(v)}"
                app:layout_constraintTop_toBottomOf="@id/iv_bookmark"
                app:layout_constraintStart_toStartOf="@id/iv_bookmark"
                app:layout_constraintEnd_toEndOf="@id/iv_bookmark"
                android:src="@drawable/iv_share" />

            <ImageView
                android:id="@+id/iv_like"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                android:contentDescription="@string/rectangle"
                app:layout_constraintTop_toBottomOf="@id/iv_share"
                app:layout_constraintStart_toStartOf="@id/iv_share"
                app:layout_constraintEnd_toEndOf="@id/iv_bookmark"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_like" />

            <ImageView
                android:id="@+id/iv_disLike"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                android:contentDescription="@string/rectangle"
                app:layout_constraintTop_toBottomOf="@id/iv_like"
                app:layout_constraintStart_toStartOf="@id/iv_like"
                app:layout_constraintEnd_toEndOf="@id/iv_like"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_dislike" />

            <ImageView
                android:id="@+id/ivCaution"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                app:layout_constraintTop_toBottomOf="@id/iv_disLike"
                app:layout_constraintStart_toStartOf="@id/iv_disLike"
                app:layout_constraintEnd_toEndOf="@id/iv_disLike"
                android:contentDescription="@string/rectangle"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_caution" />

            <ImageView
                android:id="@+id/iv_highlight"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:elevation="@dimen/_10sdp"

                android:contentDescription="@string/rectangle"
                android:onClick="@{v -> vm.onClick(v)}"
                android:src="@drawable/iv_heighlight"
                app:layout_constraintTop_toBottomOf="@id/ivCaution"
                app:layout_constraintStart_toStartOf="@id/ivCaution"
                app:layout_constraintEnd_toEndOf="@id/ivCaution"/>
<!--        </LinearLayout>-->


        <View
            android:id="@+id/linearItems"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="0dp"
            android:background="@drawable/bg_cards"
            android:backgroundTint="@color/app_blue"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingBottom="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="@id/iv_folder"
            app:layout_constraintStart_toStartOf="@id/iv_folder"
            app:layout_constraintTop_toBottomOf="@id/homeProfile"
            app:layout_constraintBottom_toBottomOf="@id/iv_highlight"/>


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvOpen"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Open"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_folder"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_folder"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookmark"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bookmark Here"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_bookmark"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_bookmark"/>



        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvShare"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Share"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_share"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_share"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvLike"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Like"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_like"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_like"/>


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvDislike"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dislike"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_disLike"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_disLike"/>



        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvHazard"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Report Hazard"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/ivCaution"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/ivCaution"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_inaccuracy"
            style="@style/TextView.Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Report Inaccuracy"
            android:textColor="@color/black"
            android:textSize="@dimen/_12ssp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="@id/iv_highlight"
            app:layout_constraintEnd_toStartOf="@id/linearItems"
            app:layout_constraintBottom_toBottomOf="@id/iv_highlight"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

